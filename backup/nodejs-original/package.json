{
  "name": "cnpj-consulta-automation",
  "version": "1.0.0",
  "description": "Automatização da consulta de CNPJ no site da Receita Federal",
  "main": "consulta-cnpj.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:performance": "jest tests/performance --verbose --detectOpenHandles",
    "logs": "tail -f logs/combined-$(date +%Y-%m-%d).log | pino-pretty",
    "logs:error": "tail -f logs/error-$(date +%Y-%m-%d).log | pino-pretty",
    "logs:app": "tail -f logs/app-$(date +%Y-%m-%d).log | pino-pretty",
    "clean": "rm -rf *.html *.json logs/ tmp/",
    "backtest": "node scripts/backtest-cnpjs.js",
    "backtest:run": "./scripts/run-backtest.sh",
    "analyze-errors": "node scripts/analyze-errors.js"
  },
  "keywords": [
    "cnpj",
    "receita-federal",
    "automation",
    "puppeteer"
  ],
  "author": "Automacao CNPJ",
  "license": "MIT",
  "dependencies": {
    "axios": "1.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "form-data": "4.0.4",
    "helmet": "^7.0.0",
    "jsdom": "^23.0.0",
    "pino": "8.21.0",
    "pino-pretty": "10.3.1",
    "pino-roll": "1.3.0",
    "puppeteer": "24.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}
